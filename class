package com.mycompany.data;

/**
 *
 * @author HP
 */
public class Employee {
    String name;
    double salary;
    Employee(String name,double salary)
    {
        this.name=name;
        this.salary=salary;
    }
    void displayDetails() 
    {
        System.out.println("name of employee is : "+name+"  and salary is: "+salary);
    }
}

